{
  "name": "zakolik-client",
  "version": "0.0.1",
  "license": "MIT",
  "engines" : { "node" : "11.4.0" },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "docker-start": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:watch": "jest --watch",
    "build:prod": "ng build --prod --vendor-chunk",
    "clean": "rimraf ./dist/",
    "server": "node ./server.js",
    "prod": "npm run clean && npm run build:prod && npm run server",
    "ci": "npm run clean && npm run format:test && ng lint && ng test --configuration=test --browsers ChromeTravisCi --code-coverage && ng e2e && npm run cy:ci && npm run accessibility && npm run build:prod -- --deploy-url /zakolik/ --base-href /zakolik",
    "release": "standard-version && git push --follow-tags origin master",
    "format:write": "prettier '{src,e2e,cypress}/**/*.{ts,json,md,scss}' --write",
    "format:test": "prettier '{src,e2e,cypress}/**/*.{ts,json,md,scss}' --list-different",
    "analyze": "npm run clean && npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/stats.json",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "precy:ci": "npm run build:prod -- --deploy-url \"/\" --base-href \"/\"",
    "cy:ci": "npm-run-all --parallel --race server \"cy:run -- --config=baseUrl=http://localhost:4000\"",
    "compodoc": "compodoc -p src/tsconfig.app.json",
    "docker:prod": "docker-compose -f Production.docker-compose.yml build",
    "docker:prod-up": "docker-compose -f Production.docker-compose.yml up",
    "docker:prod-down": "docker-compose -f Production.docker-compose.yml down"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "{src,e2e,cypress}/**/*.{ts,json,md,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.2",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "^8.0.2",
    "@angular/compiler": "^8.0.2",
    "@angular/core": "^8.0.2",
    "@angular/forms": "^8.0.2",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "^8.0.2",
    "@angular/platform-browser-dynamic": "^8.0.2",
    "@angular/router": "^8.0.2",
    "@fortawesome/angular-fontawesome": "^0.4.0",
    "@fortawesome/fontawesome-free": "^5.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.5.0",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@ngrx/effects": "^8.0.1",
    "@ngrx/entity": "^8.0.1",
    "@ngrx/router-store": "^8.0.1",
    "@ngrx/store": "^8.0.1",
    "@ngrx/store-devtools": "^8.0.1",
    "@ngx-translate/core": "^11.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "auth0-js": "^9.10.4",
    "bootstrap": "^4.1.3",
    "browser-detect": "^0.2.28",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "jasmine-marbles": "^0.6.0",
    "rxjs": "^6.3.3",
    "uuid": "^3.3.2",
    "web-animations-js": "^2.2.5",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.3",
    "@angular-extensions/testing-library": "~3.0.3",
    "@angular/cli": "^8.0.3",
    "@angular/compiler-cli": "^8.0.2",
    "@angular/language-service": "^8.0.2",
    "@bahmutov/add-typescript-to-cypress": "^2.0.0",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@compodoc/compodoc": "^1.1.7",
    "@types/auth0-js": "^9.10.2",
    "@types/jasmine": "~2.8.9",
    "@types/jasminewd2": "^2.0.5",
    "@types/jest": "^24.0.15",
    "@types/node": "^10.12.2",
    "codelyzer": "~4.5.0",
    "cypress": "^3.1.3",
    "express": "^4.16.4",
    "husky": "^1.1.3",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "24.8.0",
    "jest-preset-angular": "7.1.1",
    "lint-staged": "^7.3.0",
    "npm-run-all": "^4.1.5",
    "pa11y-ci": "^2.1.1",
    "prettier": "^1.14.3",
    "protractor": "^5.4.1",
    "rimraf": "^2.6.2",
    "standard-version": "4.2.0",
    "ts-jest": "^24.0.2",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}
